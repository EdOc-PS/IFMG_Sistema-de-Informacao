programa
{
	
	funcao inicio()
	{
		real N1, N2
		escreva("Informe 2 numeros: ")
		leia(N1, N2)
		se(N2>=N1){
			escreva(N1 +" "+ N2)
		}senao{
			escreva(N2 +" "+ N1)
		}
	}
}
