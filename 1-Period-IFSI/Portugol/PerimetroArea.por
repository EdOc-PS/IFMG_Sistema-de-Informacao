programa
{
	
	funcao inicio()
	{
		real L, A, P
		escreva("Informe o lado do quadrado: ")
		leia (L)
		A=L*L
		P=2*(L+L)
		escreva("A area do quadrado é: "+A+" e o perímetro é: "+P)
	}
}
